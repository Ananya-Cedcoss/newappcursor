t notification or drawer appears\n    const cartDrawer = page.locator('.cart-notification, .cart-drawer, [data-cart-drawer]');\n    await expect(cartDrawer).toBeVisible({ timeout: 5000 });\n    console.log('‚úÖ Cart drawer or notification is visible after adding product.');\n  });\n});\n"],"mappings":";;AAAA,IAAAA,KAAA,GAAAC,OAAA;AAEAC,UAAI,CAACC,QAAQ,CAAC,2BAA2B,EAAE,MAAM;EAC/C,MAAMC,QAAQ,GAAG,uCAAuC;EACxD,MAAMC,kBAAkB,GAAG,QAAQ;EACnC,MAAMC,mBAAmB,GAAG,uBAAuB,CAAC,CAAC;EACrD,MAAMC,eAAe,GAAG,cAAc;EAEtC,IAAAL,UAAI,EAAC,sCAAsC,EAAE,OAAO;IAAEM;EAAK,CAAC,KAAK;IAC/D;IACA,MAAMA,IAAI,CAACC,IAAI,CAACL,QAAQ,EAAE;MAAEM,SAAS,EAAE;IAAmB,CAAC,CAAC;IAE5D,MAAMC,aAAa,GAAGH,IAAI,CAACI,OAAO,CAAC,wBAAwB,CAAC;IAC5D,IAAI,MAAMD,aAAa,CAACE,SAAS,CAAC,CAAC,EAAE;MACnCC,OAAO,CAACC,GAAG,CAAC,oCAAoC,CAAC;MACjD,MAAMJ,aAAa,CAACK,IAAI,CAACX,kBAAkB,CAAC;MAC5C,MAAMG,IAAI,CAACS,KAAK,CAAC,uBAAuB,CAAC;MACzC,MAAMT,IAAI,CAACU,gBAAgB,CAAC,aAAa,CAAC;IAC5C;IAEAJ,OAAO,CAACC,GAAG,CAAC,wCAAwC,CAAC;;IAErD;IACA,MAAMP,IAAI,CAACC,IAAI,CAAC,GAAGL,QAAQ,YAAYE,mBAAmB,EAAE,EAAE;MAAEI,SAAS,EAAE;IAAmB,CAAC,CAAC;;IAEhG;IACA,MAAMS,eAAe,GAAGX,IAAI,CAACI,OAAO,CAClC,yEACF,CAAC,CAACQ,KAAK,CAAC,CAAC;IACT,MAAM,IAAAC,YAAM,EAACF,eAAe,CAAC,CAACG,WAAW,CAAC;MAAEC,OAAO,EAAE;IAAM,CAAC,CAAC;IAC7D,MAAMJ,eAAe,CAACF,KAAK,CAAC,CAAC;IAC7BH,OAAO,CAACC,GAAG,CAAC,qBAAqBR,eAAe,YAAY,CAAC;;IAE7D;IACA,MAAMiB,UAAU,GAAGhB,IAAI,CAACI,OAAO,CAAC,sDAAsD,CAAC;IACvF,MAAM,IAAAS,YAAM,EAACG,UAAU,CAAC,CAACF,WAAW,CAAC;MAAEC,OAAO,EAAE;IAAK,CAAC,CAAC;IACvDT,OAAO,CAACC,GAAG,CAAC,gEAAgE,CAAC;EAC/E,CAAC,CAAC;AACJ,CAAC,CAAC","ignoreList":[]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"ab2befd2c04246c7aaaeb15303d6af3908ad4bce","contentHash":"c2f83e211c97739836f8e35d9a838fd00cda5de5bfa285f4570cabc573ac1d24"}}